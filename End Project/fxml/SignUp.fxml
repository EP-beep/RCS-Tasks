<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXRadioButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="parentPane" prefHeight="500.0" prefWidth="600.0" style="-fx-background-color: #04566E;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.SignUpController">
   <children>
      <JFXTextField fx:id="email" focusColor="#d8dfe9" labelFloat="true" layoutX="185.0" layoutY="211.0" prefHeight="35.0" prefWidth="231.0" promptText="Enter Your Email Address" style="-fx-background-color: #598EC2;" unFocusColor="#b4d330">
         <font>
            <Font size="14.0" />
         </font>
      </JFXTextField>
      <JFXPasswordField fx:id="password" focusColor="#d8dfe9" labelFloat="true" layoutX="185.0" layoutY="290.0" prefHeight="35.0" prefWidth="231.0" promptText="Enter Your Password" style="-fx-background-color: #598EC2;" unFocusColor="#b4d330">
         <font>
            <Font size="14.0" />
         </font></JFXPasswordField>
      <JFXRadioButton fx:id="male" layoutX="201.0" layoutY="363.0" selected="true" text="Male" textFill="#b4d330">
         <font>
            <Font size="15.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="gender" />
         </toggleGroup>
      </JFXRadioButton>
      <JFXRadioButton fx:id="female" layoutX="283.0" layoutY="363.0" text="Female" textFill="#b4d330" toggleGroup="$gender">
         <font>
            <Font size="15.0" />
         </font>
      </JFXRadioButton>
      <JFXButton fx:id="signup" layoutX="186.0" layoutY="416.0" onAction="#SignUp" prefHeight="40.0" prefWidth="231.0" style="-fx-background-color: #b4d330;" text="Sign Up" textFill="#04566e">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </JFXButton>
      <ImageView fitHeight="21.0" fitWidth="26.0" layoutX="144.0" layoutY="216.0">
         <image>
            <Image url="@../../images/Contacts_52px.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="21.0" fitWidth="26.0" layoutX="144.0" layoutY="363.0">
         <image>
            <Image url="@../../images/Genderqueer_50px.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="21.0" fitWidth="26.0" layoutX="144.0" layoutY="296.0">
         <image>
            <Image url="@../../images/Lock%202_64px.png" />
         </image>
      </ImageView>
      <JFXButton fx:id="login" layoutX="480.0" layoutY="22.0" onAction="#loginAction" prefHeight="40.0" prefWidth="102.0" style="-fx-background-color: #b4d330;" text="Log In" textFill="#04566e">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </JFXButton>
      <ImageView layoutX="210.0" layoutY="62.0">
         <image>
            <Image url="@../../images/hellofreeday.outline-demo%20(1).png" />
         </image>
      </ImageView>
      <ImageView fitHeight="57.0" fitWidth="32.0" layoutX="284.0" layoutY="14.0">
         <image>
            <Image url="@../../images/man-accessory-010-512.png" />
         </image>
      </ImageView>
      <ImageView fx:id="progress" fitHeight="41.0" fitWidth="40.0" layoutX="281.0" layoutY="149.0">
         <image>
            <Image url="@../../images/unnamed%20(1).gif" />
         </image>
      </ImageView>
      <JFXRadioButton fx:id="other" layoutX="373.0" layoutY="363.0" text="Other" textFill="#b4d330" toggleGroup="$gender">
         <font>
            <Font size="15.0" />
         </font>
      </JFXRadioButton>
      <ImageView fx:id="registrationarrow" fitHeight="57.0" fitWidth="62.0" layoutX="500.0" layoutY="71.0">
         <image>
            <Image url="@../../images/icons8-up-arrow-100.png" />
         </image>
      </ImageView>
      <Label fx:id="registrationdonee" layoutX="451.0" layoutY="128.0" prefHeight="30.0" prefWidth="139.0" text="Registration successful!" textFill="#b4d330">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="registrationdone" layoutX="462.0" layoutY="155.0" prefHeight="30.0" prefWidth="117.0" text="You can now Log In" textFill="#b4d330">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
